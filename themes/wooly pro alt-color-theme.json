	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FAF8F685"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"constant",
				"constant.numeric",
				"constant.character.escape",
				"constant.language"
			],
			"settings": {
				"foreground": "#0FFF50",
			}
		},
		{
			"name": "Entity",
			"scope": [
				"entity.name",
				"entity.other",
			],
			"settings": {
				"foreground": "#FAFA33",
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#ff0000"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword.operator",
				"keyword.other",
				"keyword.control"
			],
			"settings": {
				"foreground": "#b2a0e8"
			}
		},
		{
			"name": "Meta",
			"scope": [
				"meta",
				"meta.group.simple.subexpression",
				"meta.function",
				"meta.object",
				"meta.objectliteral",
				"meta.var",
				"meta.definition",
				"meta.object-literal",
				"meta.attribute",
				"meta.scriptblock"
			],
			"settings": {
				"foreground": "#FFBF00",
				"fontStyle": "italic"
			}
		},
		{	
			"name": "Punctuation",
			"scope" : [
				"punctuation.definition",
				"punctuation.separator",
				"punctuation.section",
				"punctuation.support"
			],
			"settings": {
				"foreground": "#b2a0e8",
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#FFBF00",
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
			],
			"settings": {
				"foreground": "#fc4384",
			},
		},
		{	"scope": [
				"support.function.powershell",
				"support.function.builtin.python",
				"support.function",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"support.other.namespace.php",
				"support.type.sys-types",
			],
			"settings": {
				"foreground": "#FAFA33",
			}
		},
		{
			"scope": [
				"support.variable",
				"support.constant",
			],
			"settings": {
				"foreground": "#FFBF00",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable",
				"variable.other",
				"variable.other.readwrite",
				"variable.other.readwrite.powershell",
				"punctuation.definition.variable",
				"variable.function"
			],
			"settings": {
				"foreground": "#48D1CC",
			}
		},
		{
			"name": "Single and Double Quotes",
			"scope": [
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end",
			],
			"settings": {
				"foreground": "#FAF8F6",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"meta.function-call",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#0FFF50",
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
	]
}