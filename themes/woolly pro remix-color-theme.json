	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#E1DFDD55"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"constant",
				"constant.numeric",
				"constant.character.escape",
				"constant.language"
			],
			"settings": {
				"foreground": "#00ff9c",
			}
		},
		{
			"name": "Entity",
			"scope": [
				"entity.name",
				"entity.name.type",
				"entity.name.class",
				"entity.name.namespace",
				"entity.name.scope-resolution",
				"entity.other",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#00ff9c",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#EE4B2B"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword.operator",
				"keyword.other",
				"keyword.control"
			],
			"settings": {
				"foreground": "#808080",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Meta",
			"scope": [
				"meta",
				"meta.function",
				"meta.object",
				"meta.objectliteral",
				"meta.var",
				"meta.definition",
				"meta.object-literal",
				"meta.attribute",
				"meta.scriptblock"
			],
			"settings": {
				"foreground": "#00c3ff"
			}
		},
		{
			"scope": [
				"meta.group.simple.subexpression",
			],
			"settings": {
				"foreground": "#FF7518",
				"fontStyle": "italic"
			}
		},
		{	
			"name": "Punctuation",
			"scope" : [
				"punctuation.definition",
				"punctuation.separator",
				"punctuation.section",
				"punctuation.support"
			],
			"settings": {
				"foreground": "#ff5b5b",
			}
		},
		{
			"name": "Source",
			"scope": [
				"source.python"
			],
			"settings": {
				"foreground": "#00c3ff",
				"fontStyle": "bold"
			}
		},	
		{
			"name": "Storage",
			"scope": [
				"storage.type",
				"storage.modifier",
				
			],
			"settings": {
				"foreground": "#FF7518",
				"fontStyle": "bold"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
			],
			"settings": {
				"foreground": "#ffcc00"
			},
		},
		{	"scope": [
				"support.function.powershell",
				"support.function.builtin.python",
				"support.function",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"support.other.namespace.php",
				"support.type.sys-types",
			],
			"settings": {
				"foreground": "#E0B0FF"
			}
		},
		{
			"scope": [
				"support.variable",
				"support.constant",
			],
			"settings": {
				"foreground": "#E0B0FF",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable",
				"variable.other",
				"variable.other.readwrite",
				"variable.other.readwrite.powershell",
				"variable.function"
			],
			"settings": {
				"foreground": "#00ff9c",
			}
		},
		{
			"name": "Variable Identifiers",
			"scope": [
				"punctuation.definition.variable",
			],
			"settings": {
				"foreground": "#ff5b5b",
				"fontStyle": "bold"
			}
			},
		{
			"name": "Single and Double Quotes",
			"scope": [
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end",
			],
			"settings": {
				"foreground": "#ff5b5b",
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"meta.function-call",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#00ff9c",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
	]
}