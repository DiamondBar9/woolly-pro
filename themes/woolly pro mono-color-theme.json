	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FAF8F655"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"constant",
				"constant.numeric",
				"constant.character.escape",
				"constant.language"
			],
			"settings": {
				"foreground": "#50C878",
			}
		},
		{
			"name": "Entity",
			"scope": [
				"entity.name",
				"entity.name.type",
				"entity.name.class",
				"entity.name.namespace",
				"entity.name.scope-resolution",
				"entity.other",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#FDDE6C",
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#EE4B2B"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword.operator",
				"keyword.other",
				"keyword.control"
			],
			"settings": {
				"foreground": "#FF6961",
				//"foreground": "#faf8f695",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Meta",
			"scope": [
				"meta",
				"meta.function",
				"meta.object",
				"meta.objectliteral",
				"meta.var",
				"meta.definition",
				"meta.object-literal",
				"meta.attribute",
				"meta.scriptblock"
			],
			"settings": {
				//"foreground": "#FFAA1D",
				"foreground": "#FDDE6C"
			}
		},
		{
			"scope": [
				"meta.group.simple.subexpression",
			],
			"settings": {
				"foreground": "#FFAA1D",
				"fontStyle": "italic"
			}
		},
		{	
			"name": "Punctuation",
			"scope" : [
				"punctuation.definition",
				"punctuation.separator",
				"punctuation.section",
				"punctuation.support"
			],
			"settings": {
				"foreground": "#FF6961",
			}
		},
		{
			"name": "Source",
			"scope": [
				"source.python"
			],
			"settings": {
				"foreground": "#48D1CC",
				"fontStyle": "bold"
			}
		},	
		{
			"name": "Storage",
			"scope": [
				"storage.type",
				"storage.modifier",
				
			],
			"settings": {
				"foreground": "#b2a0e8",
				"fontStyle": "bold"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
			],
			"settings": {
				//"foreground": "#FF6961",
				"foreground": "#b2a0e8"
			},
		},
		{	"scope": [
				"support.function.powershell",
				"support.function.builtin.python",
				"support.function",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"support.other.namespace.php",
				"support.type.sys-types",
			],
			"settings": {
				//"foreground": "#FDDE6C",
				"foreground": "#FFAA1D"
			}
		},
		{
			"scope": [
				"support.variable",
				"support.constant",
			],
			"settings": {
				"foreground": "#FFAA1D",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable",
				"variable.other",
				"variable.other.readwrite",
				"variable.other.readwrite.powershell",
				"punctuation.definition.variable",
				"variable.function"
			],
			"settings": {
				"foreground": "#48D1CC",
			}
		},
		{
			"name": "Single and Double Quotes",
			"scope": [
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end",
			],
			"settings": {
				"foreground": "#FF6961",
				//"fontStyle": "bold"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"meta.function-call",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#50C878",
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
	]
}